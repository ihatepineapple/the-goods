/services/auth-services';\n\nimport Home from './Home';\nimport ProjectList from '../Projects/ProjectList';\nimport ProjectDetails from '../Projects/ProjectDetails';\nimport AddProjectForm from '../Projects/Forms/AddProjectForm';\nimport EditProjectForm from '../Projects/Forms/EditProjectForm';\nimport Profile from '../Profile/Profile';\nimport Signup from '../Auth/Signup';\nimport Login from '../Auth/Login';\nimport Navbar from \"../Navbar/Navbar\";\nimport ProtectedRoute from \"../Auth/ProtectedRoute\";\nimport EditProfileForm from \"../Profile/Forms/EditProfileForm\";\nimport ProfileFromBusiness from \"../Profile/ProfileFromBusiness\";\n\nfunction App() {\n  const [loggedInUser, setLoggedInUser] = useState(null);\n\n  const getLoggedInUser = (userObject) => {\n    setLoggedInUser(userObject);\n  };\n\n  const service = new AuthService();\n\n  const fetchUser = () => {\n    if (loggedInUser === null) {\n      service\n        .isAuthenticated()\n        .then((response) => {\n          setLoggedInUser(response);\n        })\n        .catch((err) => {\n          setLoggedInUser(false);\n        });\n    }\n  };\n\n  fetchUser();\n\n  return loggedInUser ? (\n    <section className=\"App\">\n      <Navbar userInSession={loggedInUser} getUser={getLoggedInUser} />\n      <Switch>\n        <Route \n        getUser={getLoggedInUser}\n        exact path=\"/login\" \n        render={() => <Redirect to=\"/projects\"/>}\n    \n        />\n\n  \n        <ProtectedRoute\n          user={loggedInUser}\n          exact path=\"/projects/create\"\n          component={AddProjectForm}\n        />\n        \n        <ProtectedRoute\n          user={loggedInUser}\n          exact path=\"/projects\"\n          component={ProjectList}\n        />\n\n        <ProtectedRoute\n          user={loggedInUser}\n          exact path=\"/projects/:id\"\n          component={ProjectDetails}\n        />\n\n        <ProtectedRoute\n          user={loggedInUser}\n          path=\"/projects/:id/edit\"\n          component={EditProjectForm}\n        />\n        \n        \n        <ProtectedRoute\n          user={loggedInUser}\n          exact path=\"/profile/:id\"\n          component={ProfileFromBusiness}\n        />\n\n        <ProtectedRoute\n          user={loggedInUser}\n          exact path=\"/profile\"\n          component={Profile}\n        />    \n\n        <ProtectedRoute\n          user={loggedInUser}\n          exact path=\"/profile/:id/edit\"\n          component={EditProfileForm}\n        />\n        \n      </Switch>\n    </section>\n  ) : (\n    <section className=\"App\">\n      <Navbar userInSession={loggedInUser} getUser={getLoggedInUser} />\n\n      <Switch>\n        \n        <Route\n          exact\n          path=\"/signup\"\n          render={() => <Signup getUser={getLoggedInUser} />}\n        />\n        <Route\n          exact\n          path=\"/login\"\n          render={() => <Login getUser={getLoggedInUser} />}\n        />\n        <Route\n          exact\n          path=\"/\"\n          render={() => <Home getUser={getLoggedInUser} />}\n        />\n        <ProtectedRoute\n          user={loggedInUser}\n          path=\"/projects/:id\"\n          component={ProjectDetails}\n        />\n        <ProtectedRoute\n          user={loggedInUser}\n          path=\"/projects\"\n          component={ProjectList}\n        />\n      </Switch>\n    </section>\n  );\n}\n\nexport default App;\n","/Users/jones/Desktop/Ironhack/LABS/the-goods/client/src/components/Projects/Forms/AddProjectForm.js",[],"/Users/jones/Desktop/Ironhack/LABS/the-goods/client/src/components/Projects/ProjectList.js",[],"/Users/jones/Desktop/Ironhack/LABS/the-goods/client/src/components/Root/Home.js",[],"/Users/jones/Desktop/Ironhack/LABS/the-goods/client/src/components/Projects/ProjectDetails.js",["87","88"],"import React, { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { Link } from \"react-router-dom\";\n\nconst ProjectDetails = (props) =