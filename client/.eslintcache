bmit={handleFormSubmit}>\n          <label>First Name:</label>\n          <input\n            type=\"text\"\n            name=\"firstName\"\n            value={registerForm.firstName}\n            onChange={handleChange}\n          />\n\n        <label>Last Name:</label>\n          <input\n            type=\"text\"\n            name=\"lastName\"\n            value={registerForm.lastName}\n            onChange={handleChange}\n          />\n\n        <label>Email:</label>\n          <input\n            type=\"text\"\n            name=\"email\"\n            value={registerForm.email}\n            onChange={handleChange}\n          />  \n\n        <div>\n            <label>Who are you?</label>\n            <br />\n            <div>\n                <input type=\"radio\" name=\"type\" value=\"talent\" onChange={handleChange}/>\n                <label>I am a freelancer</label>\n                <input type=\"radio\" name=\"type\" value=\"business\" onChange={handleChange}/>\n                <label>I am looking for talent</label>\n            </div>\n            {/* <select name=\"type\">\n                <option value=\"talent\">I am a freelancer</option>\n                <option value=\"business\">I am looking for talent</option>\n            </select> */}\n         \n            \n        </div>\n  \n          <label>Password:</label>\n          <input\n            type=\"password\"\n            name=\"password\"\n            value={registerForm.password}\n            onChange={handleChange}\n          />\n  \n        <button type=\"submit\">Sign Up</button>\n        </form>\n        <br />\n  \n        {registerErrorMsg && <span style={{ color: \"red\" }}>{registerErrorMsg}</span>}\n  \n        \n      </div>\n    );\n  };\n  \n  export default Signup;\n  \n","/Users/jones/Desktop/Ironhack/LABS/the-goods/client/src/services/auth-services.js",[],"/Users/jones/Desktop/Ironhack/LABS/the-goods/client/src/components/Auth/Login.js",["69","70","71"],"import React, { useState }